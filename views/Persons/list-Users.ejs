<%- include("../_header") -%>
    <h1>List of Persons (<%= model.length %>)</h1>
   
    <a class="btn btn-sm btn-success" href="/listUsers/add">Add</a>
   
    <div class="table-responsive-sm">
        <table class="table table-hover">
            <thead>
                <tr>
                    <th>FirstName</th>
                    <th>LastName</th>
                    <th>EmailAddress</th>
                    <th>phone</th>
                </tr>
            </thead>
            <tbody>
                <% for (const user of model) { %>
                <tr>
                    <td><%= user.FirstName %></td>
                    <td><%= user.LastName %></td>
                    <td><%= user.EmailAddress %></td>
                    <td><%= user.phone %></td>
                </tr>
                <% } %>
            </tbody>
        </table>


<%- include("../_footer") -%>